  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.18.0",
    "babel-eslint": "^7.0.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "^6.18.0",
    "chai": "^3.5.0",
    "codacy-coverage": "^2.0.0",
    "eslint": "^3.8.1",
    "eslint-loader": "^1.6.0",
    "istanbul": "^1.0.0-alpha.2",
